    }

    .sticky {
        /*position: fixed;*/
        top: 0;
        position: fixed;
        width: 100%;
        background-color: #fff;
        z-index: 9999;
        height: 60px;
        right: 15px;
        box-shadow: 0 2px 3px rgba(0, 0, 0, 0.15);

