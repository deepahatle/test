        background-position: center center;
        height: 500px;
        margin-top: 70px;
    }

    #banner:before {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: rgba(255, 255, 255, 0.6);
    }

