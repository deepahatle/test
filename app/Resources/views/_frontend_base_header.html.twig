    }

    .main-header .sticky {

    }

    .main-header .sticky li {
        border-top: none;
    }

    #go-top {
        position: fixed;
        bottom: 15px;
        right: 15px;
        z-index: 99;
        display: none;
    }

    #go-top a {
        width: 44px;
        display: block;
        text-align: center;
        text-decoration: none;
        color: #fff;
        -webkit-transition: 0.2s;
        transition: 0.2s;
    }

    #go-top a:hover {

    }

