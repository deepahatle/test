<!DOCTYPE html>
<!--[if IE 9]>
<html class="ie9 no-focus"> <![endif]-->
<!--[if gt IE 9]><!-->
<html class="no-focus" {% block html %}{% endblock %} > <!--<![endif]-->
<head>
    <meta charset="utf-8">

    <title>{% block title %}Welcome to Sugarlogger!{% endblock %}</title>

    <meta name="author" content="Sugarlogger">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    {% block meta %}{% endblock %}
    {% block canonical %}
        <link rel="canonical" href="{{ url(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) | lower }}" />
    {% endblock %}

    <!-- Icons -->
    <!-- The following icons can be replaced with your own, they are used by desktop and mobile browsers -->
    {% block favicon %}{% endblock %}
    <!-- END Icons -->

    <!-- Web fonts -->
    <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400italic,600,700%7COpen+Sans:300,400,400italic,600,700" rel="stylesheet">
    
    {#<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>#}

    <!-- Page JS Plugins CSS go here -->
    {% block stylesheets %}{% endblock %}
    
    <!-- Bootstrap and OneUI CSS framework -->
    <link rel="stylesheet" href="{{ asset("assets/css/bootstrap.min.css") }}">
    <link rel="stylesheet" id="css-main" href="{{ asset("assets/css/oneui.min.css") }}">
    <link rel="stylesheet" href="{{ asset("assets/css/style.css") }}">

    <!-- Custom CSS go here -->
    {% block stylesheets_custom %}{% endblock %}

    <base href="/">
</head>
<body>
<!-- Analytics Code -->
<script>
</script>
<!-- Analytics Code -->

{% block body %}{% endblock %}

{% block base_javascripts %}
    <script src="{{ asset('assets/js/core/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/js/core/bootstrap.min.js') }}"></script>
{% endblock %}

{% block javascripts %}{% endblock %}
</body>
</html>